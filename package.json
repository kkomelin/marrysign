{
  "name": "marrysign",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "network": "hardhat node",
    "compile": "hardhat compile",
    "deploy": "hardhat run --network localhost scripts/deploy.ts",
    "deploy:goerli": "hardhat run --network goerli scripts/deploy.ts",
    "deploy:mumbai": "hardhat run --network polygonMumbai scripts/deploy.ts",
    "verify:mumbai": "hardhat verify --network polygonMumbai 0xCce4880C3DAf296d87FE710953E12D008608C6A2 '0x0715A7794a1dc8e42615F059dD6e406A6594651A'",
    "lint": "solhint 'contracts/*.sol'",
    "lint:fix": "solhint 'contracts/**/*.sol' --fix",
    "test": "hardhat test",
    "test:gas": "REPORT_GAS=true hardhat test",
    "test:coverage": "hardhat coverage --solcoverjs ./.solcover.js",
    "format": "prettier --write .",
    "docs": "hardhat docgen",
    "prepare": "husky install"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.5.1",
    "@nomicfoundation/hardhat-toolbox": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.2",
    "dotenv": "^16.0.3",
    "hardhat": "^2.12.0",
    "husky": "^8.0.1",
    "lint-staged": "^13.0.3",
    "prettier-plugin-solidity": "^1.0.0-beta.24",
    "solhint": "^3.3.7",
    "solhint-plugin-prettier": "^0.0.5",
    "solidity-coverage": "^0.8.2",
    "solidity-docgen": "^0.6.0-beta.29",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "lint-staged": {
    "*.{ts,js,json,yml,yaml}": [
      "prettier --write"
    ],
    "*.sol": [
      "prettier --write"
    ]
  }
}
